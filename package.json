      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.8",
    "concurrently": "^6.3.0",
    "json-server": "^0.17.0",
    "postcss": "^7.0.39",
    "prettier": "2.4.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
